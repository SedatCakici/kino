package uebung4;

/**
 * @author Sedat Cakici
 */
public enum Alterbeschrankung {
	AB0("[Ohne Altersbeschraenkung]"), AB6("[Ab 6 Jahren]"), AB12("[Ab 12 Jahren]"), AB16("[Ab 16 Jahren]"), AB18(
			"[Ohne Jugendfreigabe]");

	private String freigabe;

	/**
	 * Konstruktor der Enum
	 * 
	 * @param freigabe
	 *            = Altersfreigabe
	 */

	private Alterbeschrankung(String freigabe) {
		this.setAlterbeschrankung(freigabe);
	}
	/**
	 * Setter-Methode fuer die Altersfreigabe
	 * 
	 * @param freigabe
	 *            = Angepasste Altersfreigabe
	 */
	private void setAlterbeschrankung(String freigabe) {
		this.freigabe = freigabe;
	}

	/**
	 * Getter-Methode fuer die Altersfreigabe
	 * 
	 * @return
	 */
	public String getAlterbeschrankung() {
		return freigabe;
	}

}
